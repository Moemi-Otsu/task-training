<h1>ログイン</h1>

<%= form_with(scope: :session, url: sessions_path, local:true) do |f| %>

  <%= f.label :'email', t('view.user_login_email') %>
  <%= f.email_field :email %>

  <%= f.label :'password', t('view.user_login_password') %>
  <%= f.password_field :password %>

  <%= f.submit t('view.login') %>

<% end %>